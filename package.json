{
  "name": "udata-tabular-preview",
  "version": "1.0.0",
  "description": "udata tabular preview plugin",
  "directories": {
    "test": "tests"
  },
  "source": "udata-tabular-preview/index.js",
  "target": {
    "browser": {
      "includeNodeModules": {
        "vue": false
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf .parcel-cache",
    "build": "npm run clean && parcel build --dist-dir udata_tabular_preview/static/udata-tabular-preview/js/",
    "prebuild": "npm run clean",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.7.0",
    "parcel": "^2.6.0",
    "rimraf": "^3.0.2",
    "udata-front": "file:../udata-front/udata-front",
    "vue": "file:../udata-front/node_modules/vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendatateam/udata-tabular-preview.git"
  },
  "author": "Open Data Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opendatateam/udata-tabular-preview/issues"
  },
  "alias": {
    "udata-front": "../udata-front/udata-front",
    "vue": "../udata-front/node_modules/vue/dist/vue.esm-bundler.js"
  },
  "browserslist": "> 0.1% in FR, last 15 versions, Firefox ESR, not dead",
  "homepage": "https://github.com/opendatateam/udata-tabular-preview#readme"
}
